{
	"name": "chatbot-content-backend",
	"version": "0.0.1",
	"description": "",
	"author": "",
	"bugs": {
		"url": "https://github.com/Coderfy-com/chatbot-letters/issues"
	},
	"homepage": "https://github.com/Coderfy-com/chatbot-letters#readme",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"lint": "eslint . --fix",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "npm i env-cmd cross-env && cross-env NODE_ENV=development env-cmd -f .env node --max-old-space-size=7168 dist/main",
		"test": "jest",
		"test:watch": "jest --watchAll",
		"test:cov": "jest --coverage",
		"build": "nest build",
		"migration:generate": "ts-node ./node_modules/typeorm/cli migration:generate -d src/db/dataSource.ts",
		"migrate": "ts-node ./node_modules/typeorm/cli migration:run -d src/db/dataSource.ts",
		"migration:generate:aista": "ts-node ./node_modules/typeorm/cli migration:generate -d src/db/aistaDataSource.ts",
		"migrate:aista": "ts-node ./node_modules/typeorm/cli migration:run -d src/db/aistaDataSource.ts",
		"migration:generate:test": "ts-node ./node_modules/typeorm/cli migration:generate -d src/db/testDataSource.ts",
		"migrate:test": "ts-node ./node_modules/typeorm/cli migration:run -d src/db/testDataSource.ts",
		"prepare": "husky install"
	},
	"dependencies": {
		"@hapi/joi": "^17.1.1",
		"@nestjs/cache-manager": "^2.1.0",
		"@nestjs/common": "^9.4.3",
		"@nestjs/config": "^2.3.1",
		"@nestjs/core": "^9.0.0",
		"@nestjs/jwt": "^10.0.3",
		"@nestjs/mongoose": "^9.2.2",
		"@nestjs/passport": "^9.0.3",
		"@nestjs/platform-express": "^9.0.0",
		"@nestjs/schedule": "^2.2.1",
		"@nestjs/swagger": "^6.3.0",
		"@nestjs/typeorm": "^9.0.1",
		"@sendgrid/mail": "^7.7.0",
		"@types/puppeteer": "^7.0.4",
		"assert": "^2.0.0",
		"axios": "^1.3.5",
		"bcrypt": "^5.1.0",
		"cache-manager": "^5.2.3",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.0",
		"cross-env": "^7.0.3",
		"csv": "^6.3.1",
		"dotenv": "^16.3.1",
		"env-cmd": "^10.1.0",
		"fs-extra": "^11.1.1",
		"google-auth-library": "^8.9.0",
		"i": "^0.3.7",
		"mongoose": "^7.0.3",
		"mustache": "^4.2.0",
		"node-xlsx": "^0.23.0",
		"npm": "^9.6.3",
		"openai": "^3.2.1",
		"papaparse": "^5.4.1",
		"passport": "^0.6.0",
		"passport-facebook": "^3.0.0",
		"passport-github": "^1.1.0",
		"passport-google-oauth20": "^2.0.0",
		"passport-jwt": "^4.0.1",
		"pg": "^8.11.0",
		"puppeteer": "^20.9.0",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^7.2.0",
		"sib-api-v3-sdk": "^8.5.0",
		"speakeasy": "^2.0.0",
		"telegraf": "^4.12.2",
		"typeorm": "^0.3.15",
		"uuid": "^9.0.0",
		"viber-bot": "^1.0.18",
		"xlsx": "^0.19.3"
	},
	"devDependencies": {
		"@faker-js/faker": "^8.0.2",
		"@nestjs/cli": "^9.0.0",
		"@nestjs/schematics": "^9.0.0",
		"@nestjs/testing": "^9.0.0",
		"@types/bcrypt": "^5.0.0",
		"@types/express": "^4.17.13",
		"@types/jest": "29.2.4",
		"@types/multer": "^1.4.7",
		"@types/node": "18.11.18",
		"@types/passport-jwt": "^3.0.8",
		"@types/sequelize": "^4.28.14",
		"@types/supertest": "^2.0.11",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.0.0",
		"eslint": "^8.39.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-config-standard-with-typescript": "^34.0.1",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-n": "^15.7.0",
		"eslint-plugin-no-comments": "^1.1.10",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-promise": "^6.1.1",
		"husky": "^8.0.3",
		"jest": "29.3.1",
		"lint-staged": "^13.2.2",
		"prettier": "2.8.7",
		"source-map-support": "^0.5.20",
		"supertest": "^6.1.3",
		"ts-jest": "29.0.3",
		"ts-loader": "^9.2.3",
		"ts-node": "^10.0.0",
		"tsconfig-paths": "4.1.1",
		"typescript": "^4.9.5"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"moduleNameMapper": {
			"@/(.*)": "<rootDir>/src/$1",
			"@modules/(.*)": "<rootDir>/src/modules/$1",
			"@utils/(.*)": "<rootDir>/src/utils/$1",
			"@db/(.*)": "<rootDir>/src/db/$1",
			"@configs/(.*)": "<rootDir>/src/configs/$1",
			"@shared/(.*)": "<rootDir>/src/shared/$1"
		},
		"rootDir": ".",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
}
